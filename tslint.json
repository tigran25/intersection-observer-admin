{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true
      }
    ],
    "interface-over-type-literal": false,
    "ban-types": false,
    "array-type": false,
    "no-empty": false
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ]
}
